{
    "apache-access": {
        "display": "Apache Access Log",
        "path": "\/var\/log\/apache2\/access.log",
        "refresh": 0,
        "max": 10,
        "notify": false,
        "format": {
            "type": "NCSA",
            "regex": "|^((\\S*) )*(\\S*) (\\S*) (\\S*) \\[(.*)\\] \"(\\S*) (.*) (\\S*)\" ([0-9]*) (.*)( \"(.*)\" \"(.*)\"( [0-9]*/([0-9]*))*)*$|U",
            "export_title": "URL",
            "match": {
                "Date": 6,
                "IP": 3,
                "CMD": 7,
                "URL": 8,
                "Code": 10,
                "Size": 11,
                "Referer": 13,
                "UA": 14,
                "User": 5,
                "\u03bcs": 16
            },
            "types": {
                "Date": "date:H:i:s",
                "IP": "ip:geo",
                "URL": "txt",
                "Code": "badge:http",
                "Size": "numeral:0b",
                "Referer": "link",
                "UA": "ua:{os.name} {os.version} | {browser.name} {browser.version}\/100",
                "\u03bcs": "numeral:0,0"
            },
            "exclude": {
                "URL": [
                    "\/favicon.ico\/",
                    "\/\\.pml\\.php.*$\/"
                ],
                "CMD": [
                    "\/OPTIONS\/"
                ]
            }
        }
    }
}